component {	// When WireBox sees this property, it will create the HelloWorldService and inject a reference.	property name='HelloWorldService' inject='HelloWorldService';	// Default Action	function index(event, rc, prc) {				// Get the message from our service and pass it to our view via the private request collection		prc.message = HelloWorldService.sayHello();						// The view "/views/Main/index.cfm" will be run by convention  	}}